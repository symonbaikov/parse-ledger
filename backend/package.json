{
  "name": "backend",
  "version": "1.0.0",
  "description": "FinFlow Backend - Bank statements processing system",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "start": "node dist/main",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "npm run test:unit",
    "test:unit": "jest --config jest.unit.config.ts",
    "test:e2e": "jest --config jest.e2e.config.ts",
    "test:ci": "npm run test:unit -- --runInBand && npm run test:e2e",
    "test:watch": "jest --config jest.unit.config.ts --watch",
    "test:cov": "jest --config jest.unit.config.ts --coverage",
    "typeorm": "typeorm-ts-node-commonjs -d src/data-source.ts",
    "migration:run": "ts-node -r tsconfig-paths/register scripts/run-migrations.ts",
    "migration:generate": "npm run typeorm migration:generate",
    "migration:revert": "npm run typeorm migration:revert",
    "create-admin": "ts-node -r tsconfig-paths/register scripts/create-admin.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@google/generative-ai": "^0.11.4",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.9",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@react-email/components": "^1.0.4",
    "@react-email/render": "^2.0.2",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "csv-parser": "^3.2.0",
    "googleapis": "^166.0.0",
    "multer": "^2.0.2",
    "openai": "^4.104.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pdf2table": "^0.0.2",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "reflect-metadata": "^0.2.2",
    "resend": "^6.7.0",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.27",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5"
  },
